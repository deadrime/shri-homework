{
  "env": {
    "browser": true,
    "amd": true,
    "node": true
  },
  "extends": [
    "plugin:vue/essential",
    "@vue/typescript"
  ],
  "rules": {
    "vue/require-default-prop": 0,
    "vue/require-prop-types": 0,
    "import/no-unresolved": 0,
    "import/no-unassigned-import": 0,
    "quotes": [
      "warn",
      "single"
    ],
    "semi": [
      "error",
      "never"
    ],
    "no-console": "off"
  },
  "parserOptions": {
    "parser": "typescript-eslint-parser",
    "ecmaVersion": 2016
  }
}